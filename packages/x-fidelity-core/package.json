    "eslint": "^9.21.0",
    "jest": "^30.0.2",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.7.2"
  },
