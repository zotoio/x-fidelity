{
  "name": "@x-fidelity/core",
  "version": "0.0.0-semantically-released",
  "description": "Core analysis engine with universal plugin logging system and performance monitoring",
  "private": true,
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": "./dist/*"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "build": "yarn clean && tsc",
    "build:watch": "tsc --watch --sourceMap",
    "test": "cd ../../ && yarn workspace @x-fidelity/types build && cd packages/x-fidelity-core && yarn lint:fix && NODE_ENV=test jest --config jest.config.js",
    "test:unit": "cd ../../ && yarn workspace @x-fidelity/types build && cd packages/x-fidelity-core && yarn lint:fix && NODE_ENV=test jest --config jest.config.js",
    "test:coverage": "NODE_ENV=test jest --config jest.config.js --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": "git@github.com:zotoio/x-fidelity.git",
  "author": "wyvern8 <io@zoto.io>",
  "license": "MIT",
  "engines": {
    "node": ">=22.16.0",
    "yarn": ">=1.22.0"
  },
