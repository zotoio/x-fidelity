{
  "name": "@x-fidelity/core",
  "version": "0.0.0-semantically-released",
  "description": "Core analysis engine with universal plugin logging system and performance monitoring",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": "./dist/*"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "test": "yarn check-types && yarn lint && jest",
    "test:unit": "yarn check-types && yarn lint && jest",
    "test:integration": "yarn check-types && yarn lint && jest --testMatch='**/*.integration.test.ts' --passWithNoTests",
    "test:integration:binary": "yarn check-types && yarn lint && cross-env JEST_INTEGRATION=true jest src/utils/binaryDiscovery.integration.test.ts --testTimeout=30000 --verbose",
    "test:coverage": "yarn check-types && yarn lint && jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check-types": "tsc --noEmit"
  },
  "repository": "git@github.com:zotoio/x-fidelity.git",
  "author": "wyvern8 <io@zoto.io>",
  "license": "MIT",
  "engines": {
    "node": ">=22.16.0",
    "yarn": ">=1.22.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/esprima": "^4.0.6",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.5",
    "@types/nodemailer": "^6.4.17",
    "@types/prettyjson": "^0.0.33",
    "@types/yauzl": "^2.10.3",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "rimraf": "^6.1.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/types": "^7.23.0",
    "@types/body-parser": "^1.19.5",
    "@types/vscode": "^1.106.1",
    "@x-fidelity/types": "file:../x-fidelity-types",
    "@yarnpkg/lockfile": "^1.1.0",
    "axios": "^1.13.2",
    "body-parser": "^1.20.3",
    "commander": "^11.0.0",
    "dotenv": "^16.4.7",
    "esprima": "^4.0.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "fs-extra": "^11.3.2",
    "glob": "11.0.3",
    "helmet": "^8.0.0",
    "html-to-text": "^9.0.5",
    "json-rules-engine": "^7.3.1",
    "jsonpath-plus": "^10.3.0",
    "kill-port": "^2.0.1",
    "lodash": "^4.17.21",
    "nodemailer": "^6.10.0",
    "openai": "^4.86.1",
    "prettyjson": "^1.2.5",
    "sanitize-filename": "^1.6.3",
    "semver": "^7.7.3",
    "winston": "^3.18.3",
    "yaml": "^2.8.2"
  },
  "files": [
    "dist/**/*"
  ]
}
