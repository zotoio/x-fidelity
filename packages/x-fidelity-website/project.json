{
  "name": "website",
  "projectType": "application",
  "sourceRoot": "packages/x-fidelity-website/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/build"],
      "options": {
        "command": "yarn build",
        "cwd": "packages/x-fidelity-website"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn start",
        "cwd": "packages/x-fidelity-website"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn start",
        "cwd": "packages/x-fidelity-website"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn test",
        "cwd": "packages/x-fidelity-website"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn lint",
        "cwd": "packages/x-fidelity-website"
      }
    }
  },
  "tags": ["scope:website"]
} 