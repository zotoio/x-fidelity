            const mockedFsPromises = jest.mocked(fs.promises, { shallow: true });
            mockedFsPromises.readFile.mockResolvedValue(mock
    
    RuleContent);
            (path.join as jest.Mock).mockReturnValue('/path/to/openaiRule-rule.json');
    
